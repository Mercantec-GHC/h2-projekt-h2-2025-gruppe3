@page "/rooms"
@using System.Net.Http

<h1 class="mb-4">Dashboard</h1>


<div class="row">
    <div class="col-12">
        <RoomTable RoomGetDtos="@RoomGetDtos" />
    </div>
</div>



@code {
    RoomGetDto[]? RoomGetDtos;

    protected override async Task OnInitializedAsync()
    {
        RoomGetDtos = await ApiService.GetRoomsAsync(0);
    }
}